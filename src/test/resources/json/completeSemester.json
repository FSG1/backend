  "lifecycle_activities": [
    {
      "lifecycle_activity_id": 1,
      "lifecycle_activity_name": "Manage",
      "lifecycle_activity_description": "Management and control of all activities aimed at the process of development and deployment of IT systems and of IT service management."
    },
    {
      "lifecycle_activity_id": 2,
      "lifecycle_activity_name": "Analyse",
      "lifecycle_activity_description": "The analysis of relationships between processes, products and data flows within the context of the environment."
    },
    {
      "lifecycle_activity_id": 3,
      "lifecycle_activity_name": "Advise",
      "lifecycle_activity_description": "Advice regarding the reorganisation of processes and \/ or data flows. Advise on new IT systems to be developed or purchased on the basis of an analysis and in consultation with stakeholders."
    },
    {
      "lifecycle_activity_id": 4,
      "lifecycle_activity_name": "Design",
      "lifecycle_activity_description": "Design of an IT system on the basis of set specifications, and within predefined frameworks."
    },
    {
      "lifecycle_activity_id": 5,
      "lifecycle_activity_name": "Implement",
      "lifecycle_activity_description": "Build an IT system on the basis of a specific design and within a set framework."
    },
    {
      "lifecycle_activity_id": 6,
      "lifecycle_activity_name": "Professional Behaviour",
      "lifecycle_activity_description": "Show professional behaviour"
    },
    {
